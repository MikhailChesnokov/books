@model Dictionary<string, string>

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>Result</title>
</head>
<body>
<table>
    @foreach (var pair in Model)
    {
        <tr>
            <th>@pair.Key</th><td>@pair.Value</td>
        </tr>
    }
</table>
</body>
</html>